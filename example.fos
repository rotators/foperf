// Run with: ASCompiler example.fos -run foperf::main

#define FOPERF_TESTCASE @test1, @test2

#include "foperf.fos"

string test1()
{
  string s = "3.1415";
  float f = 0;
  FOPERF_START;
  f = s.toFloat();
  FOPERF_STOP;
  return "string::toFloat";
}

string test2()
{
  string s = "3.1415";
  float f = 0;
  FOPERF_START;
  StrToFloat(s, f);
  FOPERF_STOP;
  return "StrToFloat";
}
